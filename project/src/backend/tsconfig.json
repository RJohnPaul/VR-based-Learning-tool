{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true, // Ensure this flag is set to true
    "skipLibCheck": true // Optional: This can help avoid type checking issues with third-party libraries
  },
  "include": ["./**/*.ts", "session.d.ts"],
  "exclude": ["node_modules"]
}